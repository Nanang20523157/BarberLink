<?xml version="1.0" encoding="utf-8"?>
<rotate xmlns:android="http://schemas.android.com/apk/res/android"
    android:fromDegrees="90"
    android:toDegrees="90"
    android:pivotX="50%"
    android:pivotY="50%">
    <shape
        android:shape="line">
        <stroke
            android:width="1.9dp"
            android:color="#000000"
            android:dashWidth="7dp"
            android:dashGap="5dp" />
    </shape>
</rotate>